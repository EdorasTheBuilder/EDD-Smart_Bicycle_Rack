from multiprocessing import Process, Queue


check = False

def f(queue, check):
    if check == True:
        queue.put([42, None, 'hello'])
    else: 
        queue.put([False])
    


if __name__ == '__main__':
    queue = Queue()
    p = Process(target=f, args=(queue, check))
    p.start()
    print(queue.get())    # prints "[42, None, 'hello']"
    
    p.terminate()